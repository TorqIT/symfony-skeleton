{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "subscriptionId": {
      "value": "TODO-subscription-id-here"
    },
    "tenantId": {
      "value": "TODO-tenant-id-here"
    },
    "resourceGroupName": {
      "value": "My-Application-Prod"
    },
    "servicePrincipalName": {
      "value": "my-application-sp-prod"
    },
    "location": {
      "value": "canadacentral"
    },
    "keyVaultName": {
      "value": "my-application-kv-prod"
    },
    "keyVaultGenerateRandomSecrets": {
      "value": true
    },
    "containerRegistryName": {
      "value": "myapplicationcrprod"
    },
    "containerRegistrySku": {
      "value": "Basic"
    },
    "virtualNetworkName": {
      "value": "my-application-vnet-prod"
    },
    "virtualNetworkPrivateEndpointsSubnetName": {
      "value": "private-endpoints"
    },
    "storageAccountName": {
      "value": "myapplicationprod"
    },
    "storageAccountFileShares": {
      "value": []
    },
    "storageAccountLongTermBackups": {
      "value": true
    },
    "databaseServerName": {
      "value": "my-application-db-prod"
    },
    "containerAppsEnvironmentName": {
      "value": "my-application-cae-prod"
    },
    "containerAppsEnvironmentUseWorkloadProfiles": {
      "value": true
    },
    "initContainerAppJobName": {
      "value": "my-application-init-prod"
    },
    "phpContainerAppExternal": {
      "value": true
    },
    "phpContainerAppName": {
      "value": "my-application-php-prod"
    },
    "phpContainerAppCustomDomains": {
      "value": []
    },
    "phpContainerAppIpSecurityRestrictions": {
      "value": []
    },
    "appDebug": {
      "value": "0"
    },
    "appEnv": {
      "value": "prod"
    },
    "additionalEnvVars": {
      "value": []
    },
    "additionalSecrets": {
      "value": []
    },
    "additionalVolumesAndMounts": {
      "value": []
    }
  }
}
